{
  "manifest_version": 2,
  "version": "0.1.8",
  "name": "Bookmarks Commander",
  "description": "A fast two-pane bookmarks manager which is similar to NC or Total Commander.",
  "permissions": [
    "bookmarks",
    "contextMenus",
    "notifications",
    "storage",
    "clipboardWrite"
  ],
  "homepage_url": "https://add0n.com/bookmarks-commander.html",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "data/icons/16.png",
    "19": "data/icons/19.png",
    "32": "data/icons/32.png",
    "38": "data/icons/38.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png",
    "256": "data/icons/256.png",
    "512": "data/icons/512.png"
  },
  "browser_action": {
    "default_icon": "data/icons/svgs/default.svg",
    "theme_icons": [{
      "light": "data/icons/svgs/light.svg",
      "dark": "data/icons/svgs/dark.svg",
      "size": 19
    }]
  }
}
